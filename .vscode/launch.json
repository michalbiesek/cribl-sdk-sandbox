{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Control Plane: Cloud Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/control-plane/python/example_cloud.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "Python Control Plane: On-Prem Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/control-plane/python/example_onprem.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "Python Mgmt Plane: Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/mgmt-plane/python/example.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "TS Control Plane: Cloud Example",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": ["tsx", "${workspaceFolder}/examples/control-plane/typescript/example_cloud.ts"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "TS Control Plane: On-Prem Example",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": ["tsx", "${workspaceFolder}/examples/control-plane/typescript/example_onprem.ts"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "TS Mgmt Plane: Example",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "runtimeArgs": ["tsx", "${workspaceFolder}/examples/mgmt-plane/typescript/example.ts"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Go Control Plane: Cloud Example",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/control-plane/go/example_cloud.go",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/control-plane/go",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Go Control Plane: On-Prem Example",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/control-plane/go/example_onprem.go",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/control-plane/go",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Go Mgmt Plane: Example",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/examples/mgmt-plane/go/example.go",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/examples/mgmt-plane/go",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
